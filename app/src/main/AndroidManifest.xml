            android:name="com.example.broadcastreceiver.AnotherBroadcastReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.broadcastreceiver.MY_BROADCAST" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.example.broadcastreceiver.MyBroadcastReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter android:priority="100">
                <action android:name="com.example.broadcastreceiver.MY_BROADCAST" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.example.broadcastreceiver.BootCompleteReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

